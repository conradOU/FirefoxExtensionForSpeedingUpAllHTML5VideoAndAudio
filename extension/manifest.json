{
  "manifest_version": 2,
  "name": "Audio/Video Speed Up",
  "version": "1.3",
  "description": "Speeds up playback. Affects all html5 videos and audio.",
  "icons": {
    "128": "icon128.png"
  },
  "browser_action": {
    "default_icon": "icon128.png"
  },
  "developer": {
    "name": "conrad.rzuc@gmail.com",
    "url": "https://github.com/conradOU/"
  },
  "background": {
    "scripts": [
      "background.js"
    ]
  },
  "content_scripts": [
    {
      "matches": [
        "http://*/*",
        "https://*/*",
        "file:///*/*"
      ],
	  "exclude_matches": [
        "https://plus.google.com/hangouts/*",
        "https://hangouts.google.com/*",
        "https://meet.google.com/*"
      ],
      "js": [
        "VSUScript.js"
      ],
      "run_at": "document_end"
    }
  ],
  "permissions": [
    "contextMenus",
    "tabs",
    "<all_urls>",
    "http://*/*",
    "https://*/*",
    "file:///*/*",
	"webNavigation"
  ],
  "commands": {
    "video-speed-up": {
      "suggested_key": {
        "default": "Ctrl+Shift+U",
        "mac": "Command+Shift+U"
      },
      "description": "Speed up a video, any video"
    },
    "video-speed-down": {
      "suggested_key": {
        "default": "Ctrl+Shift+Y",
        "mac": "Command+Shift+Y"
      },
      "description": "Speed down a video, any video"
    }
  }
}